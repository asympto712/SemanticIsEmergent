---
import Layout from "../layouts/Layout.astro";
const pageTitle = "Blog";
const posts = Object.values(import.meta.glob('./posts/*.md', {eager: true}));
---

<Layout pageTitle={pageTitle}>
  <div>
    <ul>
      {posts.map((post:any) => (
        <li><a href={post.url}>{post.frontmatter.title}</a></li>
      ))}
    </ul>
  </div>

</Layout>
